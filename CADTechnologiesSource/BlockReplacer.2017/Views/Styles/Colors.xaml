<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Primary Blue Color-->
    <Color
        x:Key="PrimaryBlue">#2196F3</Color>
    <SolidColorBrush
        x:Key="PrimaryBlueBrush"
        Color="{StaticResource PrimaryBlue}" />

    <!--Dark Background-->
    <Color
        x:Key="DarkBackground">#252526</Color>
    <SolidColorBrush
        x:Key="DarkBackgroundBrush"
        Color="{StaticResource DarkBackground}" />

    <!--SemiDark Background-->
    <Color
        x:Key="SemiDarkBackground">#333337</Color>
    <SolidColorBrush
        x:Key="SemiDarkBackgroundBrush"
        Color="{StaticResource SemiDarkBackground}" />

    <!--HighlightedSemiDark Background-->
    <Color
        x:Key="HighlightedSemiDarkBackground">#3F3F40</Color>
    <SolidColorBrush
        x:Key="HighlightedSemiDarkBackgroundBrush"
        Color="{StaticResource HighlightedSemiDarkBackground}" />

</ResourceDictionary>